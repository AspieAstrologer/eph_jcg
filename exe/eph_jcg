#e.calc_moon_hp;     p e.moon_hp
#e.calc_r;           p e.r
#e.calc_eps;         p e.eps
#e.calc_sun_h;       p e.sun_h
#e.calc_moon_h;      p e.moon_h
#e.calc_sun_sd;      p e.sun_sd
#e.calc_moon_sd;     p e.moon_sd
