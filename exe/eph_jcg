#!/usr/bin/env ruby

require "eph_jcg"

e = EphJcg.new(ARGV)
exit unless e
p e.jst, e.utc
e.calc_all
p e.sun_ra, e.sun_dec, e.sun_dist
p e.moon_ra, e.moon_dec, e.moon_hp
p e.r, e.eps
p e.sun_h, e.moon_h
p e.sun_sd, e.moon_sd
p e.sun_lambda, e.sun_beta
p e.moon_lambda, e.moon_beta
p e.lambda_s_m
e.display_all
